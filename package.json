{
  "name": "@docs-gen/openapi",
  "version": "0.1.0",
  "description": "A simple and flexible OpenAPI documentation generator that generates JSON file following OpenAPI specifications from your API codebase.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "types": "./src/index.d.ts"
    }
  },
  "bin": {
    "docs-gen-openapi": "./src/cli/index.js"
  },
  "files": [
    "src",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "docs-gen",
    "docs-generator",
    "openapi",
    "openapi-docs",
    "openapi-docs-generator"
  ],
  "author": "ManjeetSingh-02",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/docs-gen/openapi.git"
  },
  "bugs": {
    "url": "https://github.com/docs-gen/openapi/issues"
  },
  "homepage": "https://github.com/docs-gen/openapi#readme",
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "ajv": "^8.17.1",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^3.0.1",
    "commander": "^14.0.2",
    "kleur": "^4.1.5"
  }
}
